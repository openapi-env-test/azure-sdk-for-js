{
  "advancedOptions": {
    "createSdkPullRequests": true,
    "generationCallMode": "one-per-config",
    "breakingChangeTracking": true
  },
  "initOptions": {
    "initScript": {
      "path": "sh .scripts/automation_init.sh",
      "stderr": {
        "showInComment": "npm notice Changelog",
        "scriptError": "Computing checksum with sha256sum",
        "scriptWarning": " WARN deprecated readdir-scoped-modules"
      },
      "stdout": {
        "showInComment": "see /usr/share/doc/bash/examples/startup-files",
        "scriptError": "Downloading and installing node v18.15.0",
        "scriptWarning": "shopt -s checkwinsize"
      }
    }
  },
  "generateOptions": {
    "generateScript": {
      "path": "sh .scripts/automation_generate.sh"
    },
    "parseGenerateOutput": true
  },
  "packageOptions": {
    "breakingChangeLabel": "CI-BreakingChange-JavaScript"
  }
}
